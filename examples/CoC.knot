namespace TmVar, x, y, z : Tm
  - shift: "shift" - subst: "subst"
namespace TyVar, X, Y, Z : Ty
  - shift: "tshift" - subst: "tsubst"

sort Kind, K :=
  | Star
  | KPi x T [x]K

sort Ty, S, T, U :=
  + TVar X
  | TPi  x T1 [x]T2
  | TApp T t
  | Prp
  | Prf

sort Tm, s, t, u :=
  + Var x
  | Abs x T [x]t
  | App t1 t2
  | All x T [x]t

env Ctx, G, D :=
  + empty
  | evar : x -> T
  | etvar : X -> K
